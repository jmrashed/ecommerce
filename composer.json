{
    "name": "jmrashed/ecommerce",
    "description": "Advanced E-commerce Toolkit for Laravel Application",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "ecommerce",
        "Toolkit",
        "laravel package",
        "shopping cart",
        "payment gateway",
        "Multi-Language",
        "Jmrashed"
    ],
    "autoload": {
        "psr-4": {
            "Jmrashed\\Ecommerce\\": "src/",
            "Database\\Factories\\Jmrashed\\Ecommerce\\": "database/factories"
        }
    },
    "authors": [
        {
            "name": "RASHED",
            "email": "jmrashed@gmail.com",
            "homepage": "https://github.com/jmrashed"
        }
    ],
    "support": {
        "source": "https://github.com/jmrashed/ecommerce",
        "issues": "https://github.com/jmrashed/ecommerce/issues"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/config": "10.*|11.*",
        "illuminate/validation": "10.*|11.*",
        "illuminate/database": "10.*|11.*",
        "illuminate/support": "10.*|11.*",
        "illuminate/http": "10.*|11.*",
        "illuminate/auth": "10.*|11.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jmrashed\\Ecommerce\\EcommerceServiceProvider"
            ]
        }
    }
}